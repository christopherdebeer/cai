{
  "name": "Coin Address Inspector",
  "description": "Cryptocoin address information.",
  "version": "0.5",
  "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "permissions": [
     "tabs", "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["style.css"],
      "js": ["jquery.min.js", "contentscript.js"]
    }
  ],
  "manifest_version": 2
}
